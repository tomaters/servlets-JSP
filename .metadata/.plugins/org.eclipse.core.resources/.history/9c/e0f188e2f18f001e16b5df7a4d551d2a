<title>Insert title here</title>
</head>
<body>
	<ul>
		<%
		while(enumerationDataList.hasMoreElements()) {
			String parameterName = enumerationDataList.nextElement();
			String parameterValue = application.getInitParameter(parameterName);
		%>
		<% } %>
		<li>
			
		</li>
	</ul>
</body>
</html>